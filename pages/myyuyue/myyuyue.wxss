/* pages/myyuyue/myyuyue.wxss */
page{
  background: #eeeeee
}
.wrap{
  width: 90%;
  height: 300rpx;
  border-radius: 20rpx;
  margin: 20rpx auto;
  background: #ffffff;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  box-sizing: border-box;
  padding: 20rpx
}
.wrap_item{
  width: 100%;
  font-size: 28rpx;
  display: flex;
  justify-content: space-between
}
.empty{
  font-size: 32rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
  color: #666;
  margin-top: 200rpx
}